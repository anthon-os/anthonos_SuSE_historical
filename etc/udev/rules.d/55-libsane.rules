LABEL="libsane_scsi_rules_begin"
# Generic: SCSI device type 6 indicates a scanner
KERNEL=="sg[0-9]*", ATTRS{type}=="6", MODE="0664", GROUP="lp", ENV{libsane_matched}="yes"
